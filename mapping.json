{
  "strict": {
    "text-font-family": "Poppins-SemiBold",

    "text-heading-1-font-size": 32,
    "text-heading-1-font-family": "Poppins-Bold",

    "text-heading-2-font-size": 18,
    "text-heading-2-font-family": "PublicSans-SemiBold",

    "text-paragraph-1-font-size": 16,
    "text-paragraph-1-font-family": "PublicSans-Regular",

    "text-paragraph-2-font-size": 16,
    "text-paragraph-2-font-family": "PublicSans-Bold",

    "text-caption-1-font-size": 14,
    "text-caption-1-font-family": "Poppins-Regular"
  },
  "components": {
      "Button": {
        "appearances": {
          "filled": {
            "variantGroups": {
              "size": {
                "medium": {
                  "borderRadius": 20,
                  "textFontSize": 16
                },
                "large": {
                  "borderRadius": 20,
                  "textFontSize": 20
                }
              }
            } 
          },
          "outline": {
            "variantGroups": {
              "status": {
                "primary": {
                  "backgroundColor": "transparent"
                }
              },
              "size": {
                "medium": {
                  "borderRadius": 20,
                  "borderWidth": 2,
                  "textFontSize": 16
                },
                "large": {
                  "borderRadius": 20,
                  "borderWidth": 3,
                  "textFontSize": 20
                }
              }
            }
          }
        }
      }
    }
}